<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="XMLSchema1"
           targetNamespace="http://api.inttra.com/billoflading/v1/BillOfLading.xsd"
           elementFormDefault="qualified"
           xmlns="http://api.inttra.com/billoflading/v1/BillOfLading.xsd"
           xmlns:mstns="http://api.inttra.com/billoflading/v1/BillOfLading.xsd"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:element name="BillOfLading">
    <xs:annotation>
      <xs:documentation>XSD Schema for the Bill of Lading. The schema is intended to digitize the
        process of handling negotiable as well as non-negotiable documents. The schema is divided into
        6 main elements: Header Information - general information about the document; Contract Of Carriage -
        expresses information about term and conditions of the shipment, references, routing - transportation details,
        freight charges as well as cargo information; Receipt of Goods is intended as an immutable digital stamp
        for the receipt of goods; Title of Ownership node is a critical piece of ownership that signifies the ownership of
        the document. The information in this node is managed by the service; PDf image contains the image of the document
        in the pdf form; finally parties list all parties that participate in the bol transaction.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="HeaderInformation" type="GeneralInformationType"/>
        <xs:element name="ContractOfCarriage" type="ContractOfCarriageType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="ReceiptOfGoods" type="ReceiptOfGoodsType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="TitleOfOwnership" type="TitleType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="PdfImage" type="DocumentImageType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Parties" type="PartiesType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="BillOfLadingType" type="BillOfLadingBaseType" use="required"/>
    </xs:complexType>
  </xs:element>

  <xs:annotation>
    <xs:documentation>
      General Information about the document.
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="GeneralInformationType">
    <xs:sequence>
      <xs:element name="DeclaredValue" type="xs:integer" minOccurs="0"/>
      <!--- if value declared but no currency type, it is assumed USD -->
      <xs:element name="DeclaredValueCurrency" type="CurrencyTypeCode" minOccurs="0"/>
      <xs:element name="DocumentDate" type="xs:date"/>
      <xs:element name="LocationOfRelease" type="Location" minOccurs="0"/>
      <xs:element name="PublishedVersion" type="xs:int" minOccurs="1"/>
      <xs:element name="Version" type="xs:int" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="ContractOfCarriageType">
    <xs:sequence>
      <xs:element name="TermsAndConditions" type="TermsAndConditionsType"/>
      <xs:element name="References" type="ReferencesType"/>
      <xs:element name="TransportDetails" type="TransportationDetailsType"/>
      <xs:element name="FreightCharges" type="FreightChargesType"/>
      <xs:element name="Cargo" type="CargoType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      TermsandConditionsType is intended to manage TsCs of the Bill of Lading. It is a list of number clauses that
      could be changed via an amendment process implemented by the API
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="TermsAndConditionsType">
    <xs:sequence>
      <xs:element name="ReferenceId" type="xs:string" minOccurs="0"/>
      <xs:element name="ContractDate" type="xs:date" minOccurs="0"/>
      <xs:element name="Clause" type="ClauseType" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name = "ClauseType">
    <xs:sequence>
      <xs:element name="Content" type="xs:string"/>
    </xs:sequence>
    <xs:attribute name="ClauseNumber" type="xs:int"/>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      The References type contains the collection of all references used on the document. The allowed reference types
      are enums defined in the enum section of this schema
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="ReferencesType">
    <xs:sequence>
    <xs:element name="Reference" type="ReferenceType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ReferenceType">
    <xs:sequence>
      <xs:element name="Value" type="xs:string" minOccurs="1"/>
      <xs:element name="Type" type="ReferenceNameType" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="TransportationDetailsType">
    <xs:sequence>
      <xs:element name="Carrier" type="PartyType" minOccurs="1"/>
      <xs:element name="PlaceOfReceipt" type="Location" minOccurs="0"/>
      <!-- start location for the rate , required. If the LocationFrom = Port of Load
       (see below attribute) the port of load value will be repeated -->
      <xs:element name="PortOfLoad" type="Location" minOccurs="1"/>
      <!-- Port of Load for the Rate-->
      <xs:element name="PlaceOfDelivery" type="Location" minOccurs="0"/>
      <!-- end location for the rate , required -->
      <xs:element name="PortOfDischarge" type="Location" minOccurs="1"/>
      <xs:element name="Vessel" type="VesselType" minOccurs="1"/>
      <xs:element name="VoyageNumber" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="VesselType">
    <xs:sequence>
      <xs:element name="VesselName" type="xs:string" minOccurs="1"/>
      <xs:element name="IMONumber" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      The FreightCharges Type is a collection of charges defined in the Bill of Lading document
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="FreightChargesType">
    <xs:sequence>
      <xs:element name="Charges" type="ChargesType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="RateBasis" type="RateBasisType" minOccurs="0"/>
      <xs:element name="CalculationBasis" type="CalculationBasisType" minOccurs="0"/>
      <xs:element name="PaymentLocation" type="Location" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="ChargesType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="Item" type="Charge"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Charge">
    <xs:sequence>
      <xs:element name="Name" type="xs:string" /> <!-- name of the charge: Base, Bunker - etc -->
      <xs:element name="Code" type="xs:string" />
      <xs:element name="Cost" type="xs:decimal"/>
      <xs:element name="CurrencyCode" type="CurrencyTypeCode"/>
      <xs:element name="PaymentTerms" type="PaymentTermsType"/>
    </xs:sequence>
  </xs:complexType>


  <xs:simpleType name="RateBasisType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PerContainer"/>
      <xs:enumeration value="PerDocument"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CalculationBasisType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Unitary"/>
      <xs:enumeration value="Percentage"/>
      <xs:enumeration value="Fixed"/>
      <xs:enumeration value="Teu"/>
      <xs:enumeration value="MetricTon"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PaymentTermsType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Prepaid"/>
      <xs:enumeration value="Collect"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:annotation>
    <xs:documentation>
      The Cargo Type is a collection of cargo lines specified in the Bill of Lading document
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="CargoType">
    <xs:sequence>
      <xs:element name="CargoLine" type="CargoLineType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CargoLineType">
    <xs:sequence>
      <xs:element name="lineNumber" type="xs:int" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Equipment" type="EquipmentType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="WeightsAndVolumes" type="WeightsAndVolumesType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Goods" type="GoodsType" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="EquipmentType">
    <xs:sequence>
      <xs:element name="Container" type="ContainerType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Seal" type="SealType" minOccurs="0"/>
      <xs:element name="Mark" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ContainerType">
    <xs:sequence>
      <xs:element name="Identifier" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Code" type="ContainerTypeCode" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Description" type="xs:string" minOccurs="0"/>
      <xs:element name="IsFumigated" type="xs:boolean" minOccurs="0"/>
      <xs:element name="IsLiveReefer" type="xs:boolean" minOccurs="0"/>
      <xs:element name="WeightsAndVolumes" type="WeightsAndVolumesType" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SealType">
    <xs:sequence>
      <xs:element name="Identifier" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="SealingParty" type="SealingPartyType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="WeightsAndVolumesType">
    <xs:sequence>
      <xs:element name="NetWeight" type="WeightType" minOccurs="0"/>
      <xs:element name="TareWeight" type="WeightType" minOccurs="0"/>
      <xs:element name="GrossVolume" type="VolumeType" minOccurs="0"/>
      <xs:element name="NetVolume" type="VolumeType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name = "WeightType">
    <xs:sequence>
      <xs:element name="Unit" type="UnitOfWeightType" minOccurs="1"/>
      <xs:element name="Value" type="xs:decimal" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name = "VolumeType">
    <xs:sequence>
      <xs:element name="Unit" type="UnitOfVolumeType" minOccurs="1"/>
      <xs:element name="Value" type="xs:decimal" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="GoodsType">
    <xs:sequence>
      <xs:element name="Description" type="xs:string" minOccurs="0"/>
      <xs:element name="PackageDetail" type="PackageDetailType" maxOccurs="unbounded"/>
      <xs:element name="HazardousGoodsInformation" type="HazardousGoodsType" minOccurs="0"/>
      <xs:element name="TotalNumberFfPackages" type="xs:int" minOccurs="0"/>
      <xs:element name="TotalWeight" type="WeightType" minOccurs="0"/>
      <xs:element name="TotalVolume" type="VolumeType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PackageDetailType">
    <xs:sequence>
      <xs:element name="Quantity" type="xs:int" minOccurs="1" />
      <xs:element name="PackageTypeInformation" type="PackageType" minOccurs="0"/>
      <xs:element name="PackageMeasurements" type="WeightsAndVolumesType" minOccurs="0"/>
      <xs:element name="HSCommodityCode" type="xs:string" minOccurs="0"/>
      <xs:element name="ExportLicense" type="ExportLicenceType" minOccurs="0"/>
      <xs:element name="Marks" type="MultiLineStringType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PackageType">
    <xs:sequence>
      <xs:annotation><xs:documentation>UNECE 2 character package type code</xs:documentation></xs:annotation>
      <xs:element name="PackageCode" type="xs:string" minOccurs="0"/>
      <xs:element name="PackageDescription" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ExportLicenceType">
    <xs:sequence>
      <xs:element name="ExportLicenseNumber" type="xs:string" minOccurs="1"/>
      <xs:element name="IssueDate" type="xs:date" minOccurs="0"/>
      <xs:element name="ExpiryDate" type="xs:date" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="HazardousGoodsType">
    <xs:sequence>
      <xs:element name="IMOClassCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="IMDGPageNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="UNDGNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="FlashpointTemperature" type="FlashpointTemperatureType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="EmergencyResponseContact" type="ContactType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Comments" type="HazardousGoodsComments" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="PackingGroupCode" type="PackingGroupCodeType" use="required"/>
  </xs:complexType>

  <xs:complexType name="FlashpointTemperatureType">
    <xs:attribute name="UnitOfTemperature" type="UnitOfTemperatureType" use="required"/>
  </xs:complexType>

  <xs:complexType name="HazardousGoodsComments">
    <xs:sequence>
      <xs:element name="Content" type="xs:string" minOccurs="1"/>
      <xs:element name="CommentsType" type="HazardousGoodsCommentsType" minOccurs="1"/>
    </xs:sequence>
  </xs:complexType>



  <xs:annotation>
    <xs:documentation>
      This type expresses information about receipt of goods.
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="ReceiptOfGoodsType">
    <xs:sequence>
      <xs:element name="ReceiptLocation" type="Location" minOccurs="1" maxOccurs="1"/>
      <xs:element name="ReceivedBy" type="PartyType" minOccurs="1"/>
      <xs:element name="ReceivedLocation" type="Location"/>
      <xs:element name="ReceivedDateTime" type="xs:dateTime"/>
    </xs:sequence>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      This type contains document image of the BillofLading.
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="DocumentImageType">
    <xs:sequence>
      <xs:element minOccurs="0" name="mimetype" type="xs:string" />
      <xs:element minOccurs="0" name="filename" type="xs:string" />
      <xs:element name="content" type="xs:base64Binary" />
    </xs:sequence>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      All parties defined in this Bill of Lading. Ownership changes can only be
      done between parties defined in the document
    </xs:documentation>
  </xs:annotation>
  <xs:complexType name="PartiesType">
    <xs:sequence>
      <xs:element name="Party" type="PartyType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PartyType">
    <xs:sequence>
      <xs:element name="PartyNameType" type="PartyNameType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="OrganizationalIdentifier" type="OrganizationalIdentifierType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="OrganizationName" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Address" type="AddressType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ContactInfo" type="ContactType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="AdditionalInfo" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OrganizationalIdentifierType">
    <xs:sequence>
      <xs:element name="IdentifierName" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="IdentifierValue" type="xs:string" minOccurs="1"/>
      <xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="CountryOfOrigin" type="xs:string" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Location">
    <xs:sequence>
      <xs:element name ="UNLOCODE" type="xs:string" minOccurs="1"/>
      <xs:element name ="LocationName" type="xs:string" minOccurs="0"/>
      <xs:element name ="SubdivisionName" type="xs:string"minOccurs="0"/>
      <xs:element name ="CountryName" type="xs:string" minOccurs="0"/>
      <xs:element name ="CountryCode" type="xs:string" minOccurs="0"/>
      <xs:element name ="Continent" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AddressType">
    <xs:sequence>
      <xs:element name="Line1" type="xs:string" minOccurs="1"/>
      <xs:element name="Line2" type="xs:string" minOccurs="0"/>
      <xs:element name="LocationDetail" type="Location" minOccurs="1"/>
      <xs:element name="PostalCode" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ContactType">
    <xs:sequence>
      <xs:element name="ContactName" type="xs:string" minOccurs="1" />
      <xs:element name="TelephoneNumberOffice" type="xs:string" minOccurs="0"/>
      <xs:element name="TelephoneNumberMobile" type="xs:string" minOccurs="0"/>
      <xs:element name="TelephoneNumberOther" type="xs:string" minOccurs="0"/>
      <xs:element name="EmailAddressPrimary" type="xs:string" minOccurs="0"/>
      <xs:element name="EmailAddressSecondary" type="xs:string" minOccurs="0"/>
      <xs:element name="TwitterHandle" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:annotation>
    <xs:documentation>
      This type contains information about the owner of the document. The BoL API will
      allow ownership change of the negotiable documents betwen parties specified in this document.
    </xs:documentation>
  </xs:annotation>

  <xs:complexType name="TitleType">
    <xs:sequence>
      <xs:element name="TitleHolder" type="PartyType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="UTCDateTimeOfOwnership" type="xs:dateTime" minOccurs="1"/>
      <xs:element name="Remarks" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- utility types and enumerations -->
  <xs:complexType name="MultiLineStringType">
    <xs:sequence>
      <xs:element name="Line" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="BillOfLadingBaseType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Negotiable"/>
      <xs:enumeration value="NonNegotiable"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ReferenceNameType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="BillOfLadingNumber"/>
      <xs:enumeration value="BookingNumber"/>
      <xs:enumeration value="BrokerReferenceNumber"/>
      <xs:enumeration value="ConsigneeOrderNumber"/>
      <xs:enumeration value="ContractNumber"/>
      <xs:enumeration value="CustomerShipmentIdentifier"/>
      <xs:enumeration value="ExporterReferenceNumber"/>
      <xs:enumeration value="FreightForwarderReference"/>
      <xs:enumeration value="FederalMaritimeComNumber"/>
      <xs:enumeration value="HouseBill"/>
      <xs:enumeration value="InvoiceNumber"/>
      <xs:enumeration value="PurchaseOrderNumber"/>
      <xs:enumeration value="ShipmentID"/>
      <xs:enumeration value="ShipperIdentifyingNumber"/>
      <xs:enumeration value="TariffNumber"/>
      <xs:enumeration value="TransactionReferenceNumber"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PartyNameType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Shipper"/>
      <xs:enumeration value="Consignee"/>
      <xs:enumeration value="Carrier"/>
      <xs:enumeration value="NotifyParty"/>
      <xs:enumeration value="CarrierAgent"/>
      <xs:enumeration value="ForwardingAgent"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SealingPartyType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Carrier"/>
      <xs:enumeration value="Customs"/>
      <xs:enumeration value="QuarantineAgency"/>
      <xs:enumeration value="Shipper"/>
      <xs:enumeration value="TerminalOperator"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PackingGroupCodeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MinorDanger"/>
      <xs:enumeration value="MediumDanger"/>
      <xs:enumeration value="GreatDanger"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="HazardousGoodsCommentsType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="General"/>
      <xs:enumeration value="HazardousPlackard"/>
      <xs:enumeration value="ProperShippingName"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="UnitOfTemperatureType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CEL"/><!-- Celsius -->
      <xs:enumeration value="FAH"/><!-- Fahrenheit -->
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="UnitOfVolumeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CBM"/> <!-- Cubic Meter -->
      <xs:enumeration value="CBF"/><!-- Cubic Feet -->
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="UnitOfWeightType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="KGS"/> <!-- Kilogram -->
      <xs:enumeration value="LBS"/><!-- Pound -->
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CurrencyTypeCode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AED"/>	<!-- ("United Arab Emirates dirham"), -->
      <xs:enumeration value="AFN"/>	<!-- ("Afghan afghani"), -->
      <xs:enumeration value="ALL"/>	<!-- ("Albanian lek"), -->
      <xs:enumeration value="AMD"/>	<!-- ("Armenian dram"), -->
      <xs:enumeration value="ANG"/>	<!-- ("Netherlands Antillean guilder"), -->
      <xs:enumeration value="AOA"/>	<!-- ("Angolan kwanza"), -->
      <xs:enumeration value="ARS"/>	<!-- ("Argentine peso"), -->
      <xs:enumeration value="AUD"/>	<!-- ("Australian dollar"), -->
      <xs:enumeration value="AWG"/>	<!-- ("Aruban florin"), -->
      <xs:enumeration value="AZN"/>	<!-- ("Azerbaijani manat"), -->
      <xs:enumeration value="BAM"/>	<!-- ("Bosnia and Herzegovina convertible mark"), -->
      <xs:enumeration value="BBD"/>	<!-- ("Barbados dollar"), -->
      <xs:enumeration value="BDT"/>	<!-- ("Bangladeshi taka"), -->
      <xs:enumeration value="BGN"/>	<!-- ("Bulgarian lev"), -->
      <xs:enumeration value="BHD"/>	<!-- ("Bahraini dinar"), -->
      <xs:enumeration value="BIF"/>	<!-- ("Burundian franc"), -->
      <xs:enumeration value="BMD"/>	<!-- ("Bermudian dollar"), -->
      <xs:enumeration value="BND"/>	<!-- ("Brunei dollar"), -->
      <xs:enumeration value="BOB"/>	<!-- ("Boliviano"), -->
      <xs:enumeration value="BOV"/>	<!-- ("Bolivian Mvdol (funds code)"), -->
      <xs:enumeration value="BRL"/>	<!-- ("Brazilian real"), -->
      <xs:enumeration value="BSD"/>	<!-- ("Bahamian dollar"), -->
      <xs:enumeration value="BTN"/>	<!-- ("Bhutanese ngultrum"), -->
      <xs:enumeration value="BWP"/>	<!-- ("Botswana pula"), -->
      <xs:enumeration value="BYN"/>	<!-- ("Belarusian ruble"), -->
      <xs:enumeration value="BZD"/>	<!-- ("Belize dollar"), -->
      <xs:enumeration value="CAD"/>	<!-- ("Canadian dollar"), -->
      <xs:enumeration value="CDF"/>	<!-- ("Congolese franc"), -->
      <xs:enumeration value="CHE"/>	<!-- ("WIR Euro (complementary currency)"), -->
      <xs:enumeration value="CHF"/>	<!-- ("Swiss franc"), -->
      <xs:enumeration value="CHW"/>	<!-- ("WIR Franc (complementary currency)"), -->
      <xs:enumeration value="CLF"/>	<!-- ("Unidad de Fomento (funds code)"), -->
      <xs:enumeration value="CLP"/>	<!-- ("Chilean peso"), -->
      <xs:enumeration value="CNY"/>	<!-- ("Chinese yuan"), -->
      <xs:enumeration value="COP"/>	<!-- ("Colombian peso"), -->
      <xs:enumeration value="COU"/>	<!-- ("Unidad de Valor Real (UVR) (funds code)[7]"), -->
